package com.company;

public class Main {

    public static void main(String[] args) {
        String [][]ar = {{" "," ", "/","|"},{" ", "/", " ", "|"},{"/", "_", "_", "|"}};
        System.out.println();
        System.out.println(ar[0][0] + ar[0][1] + ar [0][2] + ar [0][3]);
        System.out.println(ar[1][0] + ar [1][1] + ar [1][2] + ar [1][3]);
        System.out.println(ar[2][0] + ar[2][1] + ar[2][2] + ar[2][3]);
    }
}
